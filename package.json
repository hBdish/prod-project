  "loki": {
    "looks-same": {
      "ignoreCaret": true
    },
    "configurations": {
      "chrome.laptop": {
        "chromeTolerance": 10,
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
