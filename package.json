{
  "name": "product-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "concurrently \"npm start\" \"npm run start:dev:server\"",
    "start": "webpack serve --env port=3000",
    "start:vite": "vite",
    "start:dev:vite": "concurrently \"npm run start:vite\" \"npm run start:dev:server\"",
    "start:dev:server": "node ./json-server/index.js",
    "build:prod": "webpack --config webpack.config.ts --env mode=production apiUrl=https://mock-back-prod.vercel.app/",
    "build:dev": "webpack --config webpack.config.ts --env mode=development",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npm run storybook:build && npm run test:ui:ci",
    "test:ui:approve": "npx loki approve",
    "test:ui:report": "npm run test:ui:genJson && npm run test:ui:genHtml",
    "test:ui:genJson": "node scripts/generate-visual-json-report.js",
    "test:ui:genHtml": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "storybook": "storybook dev -p 6006 -c ./config/.storybook",
    "storybook:build": "storybook build -c ./config/.storybook",
    "prepare": "husky install",
    "generate:slice": "node ./scripts/create-slice/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
